                                        <div class="profile">
                                            <h2 class="book-title" id="reg">Update YOUR Patient Profile</h2>
                                            <p style="text-align:center; padding-bottom:15px;">Please enter your name, DOB, phone, email, and gender.</p>
                                            <div class="css-form"
                                                 name="signUpForm"
                                                 novalidate ng-controller="demoCtrl">

                                                <div class="alert alert-danger" id="diverror" ng-show="person.Error">
                                                    <strong>Error!</strong><span ng-bind-html="person.MessageError"></span>
                                                </div>
                                                <div class="noo-form">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <span class="wpcf7-form-control-wrap patient-portal-name">
                                                                <input type="text" name="first-name"
                                                                       ng-maxlength="20" ng-minlength="2" ng-required="true" required
                                                                      value={{firstname}}  size="40"
                                                                       class="wpcf7-form-control" placeholder="First Name" id="FirstName" tabindex=1 autofocus />
                                                            </span>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <span class="wpcf7-form-control-wrap patient-portal-name">
                                                                <input type="text"
                                                                       ng-maxlength="20" ng-minlength="2" required
                                                                       name="last-name" ng-model="person.LastName"
                                                                       size="40" class="wpcf7-form-control" placeholder="Last Name" id="LastName" tabindex=2 />
                                                            </span>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <span class="wpcf7-form-control-wrap patient-portal-date">
                                                                <input type="date" name="patient-portal-date"
                                                                       ng-model="person.DateOfBirth"
                                                                       size="40" class="wpcf7-form-control"
                                                                       id="DateOfBirth" placeholder="Date Of Birth mm/dd/yyyy" tabindex=3 ng-keypress="keypress()" ng-keydown="keydown()" ng-change="change()" />
                                                            </span>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <span class="wpcf7-form-control-wrap patient-portal-phone">
                                                                <input type="number" name="patient-portal-phone"
                                                                       ng-maxlength="20" ng-minlength="4" required
                                                                       ng-model="person.Phone1" size="40" class="wpcf7-form-control" placeholder="Phone number" id="Phone1" tabindex=4 />
                                                            </span>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <span class="wpcf7-form-control-wrap patient-portal-email">
                                                                <input type="email" name="patient-portal-email"
                                                                       ng-maxlength="30" ng-minlength="6" required
                                                                       ng-model="person.Email"
                                                                       size="40" class="wpcf7-form-control" placeholder="Email" id="Email" tabindex=5 />
                                                            </span>

                                                        </div>



                                                        <div class="col-md-4">
                                                            <span class="wpcf7-form-control-wrap patient-portal-email">
                                                                <input type="email" name="patient-portal-emailrm-confi" ng-model="person.ConfirmEmail"
                                                                       ng-maxlength="30" ng-minlength="6" required
                                                                       size="40" class="wpcf7-form-control" placeholder="Confirm Email" id="ConfirmEmail" tabindex=6 />
                                                            </span>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <span class="wpcf7-form-control-wrap car">
                                                                <select name="sex" required class="wpcf7-form-control" ng-model="person.Gender" id="Gender" tabindex=7>
                                                                    <option value="">Gender</option>
                                                                    <option value="0">Male</option>
                                                                    <option value="1">Female</option>
                                                                    <option value="2">Transgender</option>
                                                                </select>
                                                            </span>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <span class="wpcf7-form-control-wrap patient-portal-email">
                                                                <input type="password"
                                                                       required
                                                                       name="patient-portal-emailrm-confi" ng-model="person.PasswordHash"
                                                                       size="40" class="wpcf7-form-control" placeholder="Password" id="PasswordHash" tabindex=8 />
                                                            </span>
                                                        </div>


                                                        <div class="col-md-4">


                                                            <span class="wpcf7-form-control-wrap patient-portal-email">
                                                                <input type="password"
                                                                       ng-maxlength="30" ng-minlength="2" required
                                                                       ng-model="person.ConfirmPassword"
                                                                       name="patient-portal-emailrm-confi"
                                                                       size="40" class="wpcf7-form-control" placeholder="Confirm Password" id="ConfirmPassword" tabindex=9 />
                                                            </span>
                                                        </div>
                                                    </div>
                                                    <!--flag-->
